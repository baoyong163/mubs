<h1>Editing site</h1>

<%= error_messages_for :site %>

<% form_for(@site) do |f| %>
  <p>
    <b>Name</b><br />
    <%= f.text_field :name %>
  </p>

  <p>
    <b>States</b><br />
    <%= f.text_field :states %>
  </p>

  <p>
    <%= f.submit :Update.t %>
  </p>
<% end %>

<%= link_to :Show.t, @site %> |
<%= link_to :Back.t, sites_path %>
