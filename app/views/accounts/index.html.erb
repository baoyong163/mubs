<h1>Listing accounts</h1>

<table>
  <tr>
  </tr>

<% for account in @accounts %>
  <tr>
    <td><%= link_to :Show.t, account %></td>
    <td><%= link_to :Edit.t, edit_account_path(account) %></td>
    <td><%= link_to :Destroy.t, account, :confirm => :'Are you sure?'.t, :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New account', new_account_path %>
