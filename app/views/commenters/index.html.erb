<h1>Listing commenters</h1>

<table>
  <tr>
  </tr>

<% for commenter in @commenters %>
  <tr>
    <td><%= link_to :Show.t, commenter %></td>
    <td><%= link_to :Edit.t, edit_commenter_path(commenter) %></td>
    <td><%= link_to :Destroy.t, commenter, :confirm => :'Are you sure?'.t, :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New commenter', new_commenter_path %>
